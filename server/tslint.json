{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent":[true, "tabs", 4],
        "interface-name":false,
        "max-line-length": [false],
        "no-console":false,
        "no-string-literal": false,
        "object-literal-sort-keys":true,
        "variable-name": [true, "allow-leading-underscore"],
        "class-name": [true]
    },
    "rulesDirectory": []
}
